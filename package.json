{
  "name": "7dtd-map",
  "version": "1.0.0",
  "description": "Map renderer for 7 Days to Die",
  "private": true,
  "scripts": {
    "build": "npm run resources && npm run js",
    "resources": "node ./tools/generate-block-index.js && node ./tools/generate-prefab-html.js",
    "js": "webpack --mode production",
    "dev-server": "webpack-dev-server --mode development --content-base docs",
    "lint": "npm run jslint && npm run htmllint && npm run csslint",
    "jslint": "eslint {js,tools}/**/*.js --fix",
    "htmllint": "htmlhint docs/**/*.html",
    "csslint": "stylelint docs/**/*.css --fix"
  },
  "author": "Keiichiro UI",
  "license": "MIT",
  "devDependencies": {
    "bunyan": "^1.8.14",
    "bunyan-format": "^0.2.1",
    "csv-parse": "^4.10.1",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "htmlhint": "^0.14.1",
    "lodash": "^4.17.15",
    "pako": "^1.0.11",
    "pngjs": "^5.0.0",
    "stream-to-blob": "^2.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "tga-js": "^1.1.0",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1",
    "xml2js": "^0.4.23"
  },
  "dependencies": {}
}
